{
  "version": 2,
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["src/**/*.ts"]
      }
    }
  ],
  "functions": {
    "api/*.ts": {
      "memory": 1024
    }
  },
  "routes": [{ "src": "/(.*)", "dest": "/src/index.ts" }]
}
